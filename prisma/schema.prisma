generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model Invoice {
  id              String @id @default(auto()) @map("_id") @db.ObjectId
  name            String
  quality         Int
  unit            String
  unit_price      Float
  description     String ?
  caculated_price Float
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt

  @@map("invoice")
}